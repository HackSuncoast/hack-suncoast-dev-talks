{
  "openapi": "3.0.0",
  "info": {
    "title": "Weather API",
    "version": "1.0.0"
  },
  "consumes": ["application/json"],
  "produces": ["application/json"],
  "paths": {
    "/weather": {
      "get": {
        "summary": "Retrieves weather",
        "description": "Retrieves the weather given a zipcode",
        "produces": ["application/json"],
        "responses": {
          "200": {
            "description": "200 Response",
            "schema": {
              "type": "object",
              "properties": {
                "code": {
                  "type": "number"
                },
                "temp": {
                  "type": "number"
                }
              }
            }
          }
        }
      }
    },
    "/user/register": {
      "get": {
        "summary": "Create a user",
        "description": "Create a user in the backend",
        "produces": ["application/json"],
        "responses": {
          "200": {
            "description": "200 response",
            "schema": {
              "type": "object",
              "items": {
                "properties": {
                  "code": {
                    "type": "number"
                  },
                  "message": {
                    "type": "string"
                  }
                }
              }
            }
          }
        }
      }
    }
  }
}
